{
    "definitions": {},
    "$id": "http://example.com/root.json",
    "type": "array",
    "title": "The Root Schema",
    "items": {
        "$id": "#/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
            "id",
            "name",
            "value"
        ],
        "properties": {
            "id": {
                "$id": "#/items/properties/id",
                "type": "string",
                "title": "The Id Schema",
                "default": "",
                "examples": [
                    "db593661-f10a-4bd8-a21e-08d795e1e273"
                ],
                "pattern": "^(.*)$"
            },
            "name": {
                "$id": "#/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                    "123456"
                ],
                "pattern": "^(.*)$"
            },
            "value": {
                "$id": "#/items/properties/value",
                "type": "integer",
                "title": "The Value Schema",
                "default": 0,
                "examples": [
                    5
                ]
            }
        }
    }
}